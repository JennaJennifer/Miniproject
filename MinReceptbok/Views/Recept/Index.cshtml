@model ReceptIndexVM[]


<div class="col-centered" style="width:80%; margin: auto">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel" style="margin-top:25px">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img class="d-block w-100" src="~/images/hamburgare.jpg" alt="First slide" height="420" width="600">
            </div>
            <div class="carousel-item">
                <img class="d-block w-100" src="~/images/korvgryta.jpg" alt="Second slide" height="420" width="600">
            </div>
            <div class="carousel-item">
                    <img class="d-block w-100" src="~/images/pizza.jpg" alt="Third slide" height="420" width="600">
            </div>
        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only"></span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only"></span>
        </a>
    </div>
</div>

